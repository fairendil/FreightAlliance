                <Setter.Value>
                    <BitmapImage BaseUri="pack://application:,,,/Telerik.Windows.Themes.VisualStudio2013;v2015.3.930.45;5803cfa389c90ce7;component/Themes/Telerik.Windows.Controls.RibbonView.xaml" UriSource="pack://application:,,,/Telerik.Windows.Controls.RibbonView;component/HelpIcon.png"/>
                </Setter.Value>
            </Setter>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type telerik:RadRibbonView}">
                        <Grid x:Name="RootPanel">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Rectangle Fill="{TemplateBinding Background}" Grid.Row="1" Grid.RowSpan="2"/>
                            <ContentPresenter x:Name="SelectedTabContent" ContentTemplate="{TemplateBinding ContentTemplate}" Height="{TemplateBinding ContentHeight}" Grid.Row="1"/>
                            <Grid x:Name="TopGrid">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <Border x:Name="titleBarBackground" Background="{TemplateBinding TitleBarBackground}" Grid.ColumnSpan="3" IsHitTestVisible="False"/>
                                <Rectangle Grid.ColumnSpan="3" Fill="{TemplateBinding TitleBarBackground}" Grid.Row="1"/>
                                <primitives:RibbonTitleBarPanel x:Name="RibbonTitleBarPanel" Grid.ColumnSpan="2" Visibility="{TemplateBinding TitleBarVisibility}">
                                    <ContentPresenter x:Name="AboveToolBarHost" ContentTemplate="{TemplateBinding ContentTemplate}" primitives:RibbonTitleBarPanel.Element="QuickAccessToolBar" VerticalAlignment="Center"/>
                                    <primitives:WindowTitle x:Name="WindowTitle" ApplicationName="{TemplateBinding ApplicationName}" primitives:RibbonTitleBarPanel.Element="WindowTitle" Style="{TemplateBinding WindowTitleStyle}" Title="{TemplateBinding Title}"/>
                                    <StackPanel x:Name="ContextualGroupsHeaders" primitives:RibbonTitleBarPanel.Element="ContextualGroupsPanel" Margin="1,0,0,0" Orientation="Horizontal"/>
                                </primitives:RibbonTitleBarPanel>
                                <primitives:RadRibbonApplicationDropDownButton x:Name="ApplicationButton" Content="{TemplateBinding ApplicationButtonContent}" DropDownButtonPosition="Right" IsOpen="{Binding IsApplicationMenuOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}" InnerCornerRadius="0" IsEnabled="{TemplateBinding IsApplicationButtonEnabled}" Grid.Row="1" Style="{TemplateBinding ApplicationButtonStyle}" SmallImage="{TemplateBinding ApplicationButtonImageSource}" Visibility="{TemplateBinding ApplicationButtonVisibility}"/>
                                <primitives:RibbonScrollViewer x:Name="TabsScrollViewer" Grid.Column="1" Margin="0,0,0,-1" Grid.Row="1">
                                    <ItemsPresenter/>
                                </primitives:RibbonScrollViewer>
                                <StackPanel Grid.Column="2" Orientation="Horizontal" Grid.Row="1">
                                    <telerik:RadRibbonToggleButton x:Name="MinimizeButton" IsChecked="{Binding IsMinimized, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}" InnerCornerRadius="1" IsEnabled="{TemplateBinding IsMinimizable}" Style="{TemplateBinding MinimizeButtonStyle}" Visibility="{TemplateBinding MinimizeButtonVisibility}"/>
                                    <telerik:RadRibbonButton x:Name="HelpButton" CommandTarget="{TemplateBinding HelpCommandTarget}" CommandParameter="{TemplateBinding HelpCommandParameter}" Command="{TemplateBinding HelpCommand}" InnerCornerRadius="1" Style="{TemplateBinding HelpButtonStyle}" SmallImage="{TemplateBinding HelpButtonImageSource}" telerik:ScreenTip.Title="Help" Visibility="{TemplateBinding HelpButtonVisibility}">
                                        <telerik:RadRibbonButton.ToolTip>
                                            <telerik:ScreenTip Description="{x:Null}" Icon="{x:Null}" Placement="Bottom" ScreenTipState="Title" Title="Help"/>
                                        </telerik:RadRibbonButton.ToolTip>
                                    </telerik:RadRibbonButton>
                                    <ContentPresenter Content="{TemplateBinding TabStripAdditionalContent}"/>
                                </StackPanel>
                                <Rectangle Grid.ColumnSpan="3" Fill="Transparent" Height="2" Margin="0,0,0,-1" Grid.RowSpan="2" VerticalAlignment="Bottom"/>
                            </Grid>
                            <ContentPresenter x:Name="BelowToolBarHost" ContentTemplate="{TemplateBinding ContentTemplate}" Grid.Row="2"/>
                            <Popup x:Name="TabContentPopup" AllowsTransparency="True" VerticalOffset="-1">
                                <ContentPresenter x:Name="SelectedTabContentPopup" ContentTemplate="{TemplateBinding ContentTemplate}" Height="{TemplateBinding ContentHeight}"/>
                            </Popup>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsHostedInRibbonWindow" Value="True">
                                <Setter Property="Visibility" TargetName="titleBarBackground" Value="Collapsed"/>
                                <Setter Property="Foreground" TargetName="WindowTitle" Value="{telerik:VisualStudio2013Resource ResourceKey={x:Static telerik:VisualStudio2013ResourceKey.MarkerBrush}}"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </UserControl.Resources>
    <Grid>
        <telerik:RadRibbonView HorizontalContentAlignment="Center"
			ApplicationButtonContent="{x:Static resource:Resources.MenuText}"
			IsBackstageOpen="False"
			IsMinimized="False"
			IsSynchronizedWithCurrentItem="True"
			IsTextSearchEnabled="False"
			ItemsSource="{Binding Tabs}"
			QuickAccessToolBarPosition="AboveRibbon" Style="{DynamicResource RadRibbonViewStyle1}">
            <telerik:RadRibbonView.ApplicationMenu>
                <telerik:ApplicationMenu>
                    <telerik:RadRibbonButton Command="{Binding ChangeLang}" Text="{x:Static resource:Resources.ChangeLocalText}" />
                </telerik:ApplicationMenu>
            </telerik:RadRibbonView.ApplicationMenu>
        </telerik:RadRibbonView>
    </Grid>
</UserControl>
