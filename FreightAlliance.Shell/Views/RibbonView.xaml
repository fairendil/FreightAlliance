            <Setter Property="WindowTitleStyle">
                <Setter.Value>
                    <Style TargetType="{x:Type primitives:WindowTitle}">
                        <Setter Property="IsTabStop" Value="False"/>
                        <Setter Property="Margin" Value="0,6"/>
                        <Setter Property="FontFamily" Value="{telerik:VisualStudio2013Resource ResourceKey={x:Static telerik:VisualStudio2013ResourceKey.FontFamily}}"/>
                        <Setter Property="FontSize" Value="{telerik:VisualStudio2013Resource ResourceKey={x:Static telerik:VisualStudio2013ResourceKey.FontSize}}"/>
                        <Setter Property="Foreground" Value="{telerik:VisualStudio2013Resource ResourceKey={x:Static telerik:VisualStudio2013ResourceKey.StrongBrush}}"/>
                        <Setter Property="VerticalAlignment" Value="Center"/>
                        <Setter Property="IsHitTestVisible" Value="False"/>
                        <Setter Property="IsActive" Value="True"/>
                        <Setter Property="HorizontalAlignment" Value="Left"/>
                        <Setter Property="Template">
                            <Setter.Value>
                                <ControlTemplate TargetType="{x:Type primitives:WindowTitle}">
                                    <primitives:WindowTitlePanel>
                                        <TextBlock x:Name="Title" Foreground="{TemplateBinding Foreground}" FontSize="{DynamicResource {x:Static telerik:VisualStudio2013ResourceKey.FontSize}}" Text="{TemplateBinding Title}" TextTrimming="CharacterEllipsis"/>
