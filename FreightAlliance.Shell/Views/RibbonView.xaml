                                        <TextBlock x:Name="ApplicationName" Foreground="{TemplateBinding Foreground}" FontSize="{DynamicResource {x:Static telerik:VisualStudio2013ResourceKey.FontSize}}" Text="{TemplateBinding ApplicationName}" TextTrimming="CharacterEllipsis"/>
